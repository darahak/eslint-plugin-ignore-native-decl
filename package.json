{
  "name": "eslint-plugin-ignore-native-decl",
  "version": "0.2.4",
  "description": "Preprocess JavaScript files to ignore forward declarations that use the `native function` notation for v8 extensions",
  "keywords": [
    "eslint",
    "eslint-plugin",
    "lint",
    "ignore",
    "native",
    "function",
    "declaration",
    "v8",
    "vm",
    "extension"
  ],
  "author": "Dara Hak <dara.hak@gmail.com>",
  "repository": {
    "type": "git",
    "url": "https://github.com/darahak/eslint-plugin-ignore-native-decl"
  },
  "bugs": {
    "url": "https://github.com/darahak/eslint-plugin-ignore-native-decl/issues"
  },
  "publishConfig": {
    "access": "public"
  },
  "main": "lib/index.js",
  "scripts": {
    "test": "jest",
    "cov": "codecov",
    "lint": "eslint --format=node_modules/eslint-formatter-pretty . || echo.",
    "format": "prettier --single-quote --write \"{__tests__/*.js,lib/**/*.js}\""
  },
  "dependencies": {
    "applause": "^1.2.2"
  },
  "devDependencies": {
    "codecov": "^1.0.1",
    "eslint": "^3.16.1",
    "eslint-config-airbnb-base": "^11.1.0",
    "eslint-formatter-pretty": "^1.1.0",
    "eslint-plugin-import": "^2.2.0",
    "eslint-plugin-jest": "^19.0.1",
    "jest": "^19.0.2",
    "prettier": "^0.22.0"
  },
  "jest": {
    "testPathIgnorePatterns": [
      "/node_modules/",
      "/fixtures/"
    ],
    "coverageDirectory": "./coverage",
    "collectCoverage": true
  },
  "engines": {
    "node": ">=4.x"
  },
  "license": "MIT"
}
