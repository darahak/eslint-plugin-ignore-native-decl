{
  "name": "eslint-plugin-ignore-native-decl",
  "version": "0.3.0",
  "description": "Preprocess JavaScript files to ignore forward declarations that use the `native function` notation for v8 extensions",
  "keywords": [
    "eslint",
    "eslint-plugin",
    "lint",
    "ignore",
    "native",
    "function",
    "declaration",
    "v8",
    "vm",
    "extension"
  ],
  "author": "Dara Hak <dara.hak@gmail.com>",
  "repository": {
    "type": "git",
    "url": "https://github.com/darahak/eslint-plugin-ignore-native-decl"
  },
  "bugs": {
    "url": "https://github.com/darahak/eslint-plugin-ignore-native-decl/issues"
  },
  "publishConfig": {
    "access": "public"
  },
  "main": "lib/index.js",
  "scripts": {
    "test": "jest",
    "cov": "codecov"
  },
  "dependencies": {},
  "devDependencies": {
    "@types/jest": "^24.0.23",
    "codecov": "^3.6.1",
    "eslint": "^6.6.0",
    "eslint-plugin-jest": "^23.0.4",
    "jest": "^24.9.0",
    "prettier": "^1.19.1"
  },
  "jest": {
    "testPathIgnorePatterns": [
      "/node_modules/",
      "/fixtures/"
    ],
    "coverageDirectory": "./coverage",
    "collectCoverage": true
  },
  "engines": {
    "node": ">=10.x"
  },
  "license": "MIT"
}
